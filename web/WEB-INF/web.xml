    </servlet-mapping>
     <servlet-mapping>    
        <servlet-name>UserModify</servlet-name>
        <url-pattern>/usermodify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>    
        <servlet-name>UserAddProgram</servlet-name>
        <url-pattern>/useraddprogram</url-pattern>
    </servlet-mapping>
    <servlet-mapping>    
        <servlet-name>InfoModifyPage</servlet-name>
