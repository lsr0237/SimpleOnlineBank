    </servlet-mapping>
     <servlet-mapping>    
        <servlet-name>Disapproval</servlet-name>
        <url-pattern>/disapprove</url-pattern>
    </servlet-mapping>
    <servlet-mapping>    
        <servlet-name>Approval</servlet-name>
        <url-pattern>/approval</url-pattern>
    </servlet-mapping>
   <servlet-mapping>  
        <servlet-name>DisapprovalProgram</servlet-name>
        <url-pattern>/disapprovalprogram</url-pattern>
    </servlet-mapping>
     <servlet-mapping>  
        <servlet-name>ApprovalProgram</servlet-name>
        <url-pattern>/approvalprogram</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OpenAccount</servlet-name>
         <url-pattern>/openacct</url-pattern>
    </servlet-mapping>
      <servlet-mapping>
        <servlet-name>CheckAccount</servlet-name>
        <url-pattern>/checkacct</url-pattern>
    </servlet-mapping>
       <servlet-mapping>
