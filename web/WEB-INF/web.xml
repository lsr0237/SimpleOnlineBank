    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>CustomerAction</servlet-name>
        <url-pattern>/customerAction</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RequestProgram</servlet-name>
        <url-pattern>/requestProgram</url-pattern>
    </servlet-mapping>
    
</web-app>
