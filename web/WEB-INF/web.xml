    </servlet-mapping>
    <servlet-mapping>    
        <servlet-name>UserList</servlet-name>
        <url-pattern>/userlist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>    
        <servlet-name>UserAddPage</servlet-name>
        <url-pattern>/useradd</url-pattern>
    </servlet-mapping>
     <servlet-mapping>    
        <servlet-name>UserDeleteConfirm</servlet-name>
        <url-pattern>/userdeleteconfirm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>    
        <servlet-name>UserDelete</servlet-name>
        <url-pattern>/userdelete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>    
        <servlet-name>UserModifyPage</servlet-name>
