<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <session-config>
        <session-timeout>
            60
        </session-timeout>
    </session-config>
    <!-- Servlet Declare Point-->
    <servlet>
        <servlet-name>home</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <jsp-file>/customerlogin.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>companyLogin</servlet-name>
        <jsp-file>/companylogin.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>customerDashboard</servlet-name>
        <jsp-file>/customerdashboard.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>bankerDashboard</servlet-name>
        <jsp-file>/bankerdashboard.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>adminDashboard</servlet-name>
        <jsp-file>/admindashboard.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>CustomerAuth</servlet-name>
        <jsp-file>/auth/cauth.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>EmployeeAuth</servlet-name>
        <jsp-file>/auth/eauth.jsp</jsp-file>
    </servlet>
     <servlet>
        <servlet-name>Logout</servlet-name>
        <jsp-file>/auth/logout.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>UserList</servlet-name>
        <jsp-file>/userlist.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>UserAddPage</servlet-name>
        <jsp-file>/useraddpage.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>UserDeleteConfirm</servlet-name>
        <jsp-file>/userdeleteconfirm.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>UserDelete</servlet-name>
        <jsp-file>/admin/userdelete.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>UserModifyPage</servlet-name>
        <jsp-file>/usermodifypage.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>UserModify</servlet-name>
        <jsp-file>/admin/usermodify.jsp</jsp-file>
    </servlet>
     <servlet>
        <servlet-name>UserAddProgram</servlet-name>
        <jsp-file>/admin/useraddprogram.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>InfoModifyPage</servlet-name>
        <jsp-file>/infomodifypage.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>InfoModifyProgram</servlet-name>
        <jsp-file>/infomodifyprogram.jsp</jsp-file>
    </servlet>
      <servlet>
        <servlet-name>Disapproval</servlet-name>
        <jsp-file>/bankerdisapprove.jsp</jsp-file>
    </servlet>
     <servlet>
        <servlet-name>Approval</servlet-name>
        <jsp-file>/bankerapproval.jsp</jsp-file>
    </servlet>
     <servlet>
        <servlet-name>DisapprovalProgram</servlet-name>
        <jsp-file>/banker/disapproval.jsp</jsp-file>
    </servlet>
     <servlet>
        <servlet-name>ApprovalProgram</servlet-name>
        <jsp-file>/banker/approval.jsp</jsp-file>
    </servlet>
     <servlet>
        <servlet-name>OpenAccount</servlet-name>
        <jsp-file>/customer/openaccount.jsp</jsp-file>
    </servlet>
     <servlet>
        <servlet-name>CheckAccount</servlet-name>
        <jsp-file>/checkaccount.jsp</jsp-file>
    </servlet>
     <servlet>
        <servlet-name>SaveAccount</servlet-name>
        <jsp-file>/saveaccount.jsp</jsp-file>
    </servlet>
     <servlet>
        <servlet-name>CustomerAction</servlet-name>
        <jsp-file>/customerActionForm.jsp</jsp-file>
    </servlet>
     <servlet>
        <servlet-name>RequestProgram</servlet-name>
        <jsp-file>/customer/requestprogram.jsp</jsp-file>
    </servlet>  
    <!-- Servlet Mapping Declare Point-->
    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>*.js</url-pattern>
        <url-pattern>*.css</url-pattern>
        <url-pattern>*.jpg</url-pattern>
        <url-pattern>*.gif</url-pattern>
        <url-pattern>*.png</url-pattern>
        <url-pattern>*.ico</url-pattern>
        <url-pattern>*.swf</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>companyLogin</servlet-name>
        <url-pattern>/company</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>customerDashboard</servlet-name>
